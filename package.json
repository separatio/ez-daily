{
  "name": "daily-roulette",
  "version": "0.2.0",
  "author": "Alex Radulescu",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:dev:down": "docker-compose -f docker-compose.dev.yml down",
    "lint": "next lint",
    "migrate": "dotenv -e .env.migrations -- yarn prisma migrate dev",
    "typecheck": "tsc --noEmit",
    "prettier": "prettier --write .",
    "static-checks": "yarn prettier && yarn lint && yarn typecheck",
    "generate": "yarn generate:prisma && yarn generate:nexus",
    "generate:prisma": "yarn prisma generate",
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "generate:nexus": "ts-node --transpile-only -P nexus.tsconfig.json pages/api"
  },
  "dependencies": {
    "@apollo/client": "^3.6.4",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.8.0",
    "@mui/material": "^5.8.0",
    "@next-auth/prisma-adapter": "^1.0.3",
    "@prisma/client": "^3.14.0",
    "apollo-server-micro": "^3.7.0",
    "express": "^4.18.1",
    "graphql": "^16.5.0",
    "graphql-scalars": "^1.17.0",
    "graphql-tag": "^2.12.6",
    "micro": "^9.3.4",
    "micro-cors": "^0.1.1",
    "next": "^12.1.6",
    "next-auth": "^4.3.4",
    "nexus": "^1.3.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@types/micro-cors": "^0.1.2",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "eslint": "^8.16.0",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-material-ui": "^1.0.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "prettier": "2.6.2",
    "prisma": "^3.14.0",
    "ts-node": "^10.8.0",
    "typescript": "^4.6.4"
  }
}
