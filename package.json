{
  "name": "daily-roulette",
  "version": "0.2.0",
  "author": "Alex Radulescu",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "docker:dev": "docker compose -f docker-compose.dev.yml up --build",
    "docker:dev:down": "docker compose -f docker-compose.dev.yml down",
    "lint": "next lint",
    "migrate:dev": "yarn prisma migrate dev",
    "migrate:prod": "yarn prisma migrate deploy",
    "typecheck": "tsc --noEmit",
    "prettier": "prettier --write .",
    "static-checks": "yarn prettier && yarn lint && yarn typecheck"
  },
  "dependencies": {
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.1",
    "@mui/material": "^5.2.3",
    "@next-auth/prisma-adapter": "^1.0.0",
    "@prisma/client": "^3.6.0",
    "next": "^12.0.7",
    "next-auth": "^4.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/node": "^16.11.12",
    "@types/react": "^17.0.37",
    "eslint": "^8.4.0",
    "eslint-config-next": "^12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-material-ui": "^1.0.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "2.5.1",
    "prisma": "^3.6.0",
    "typescript": "^4.5.3"
  }
}
