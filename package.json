{
  "name": "ez-daily",
  "version": "0.2.0",
  "author": "Alex Radulescu",
  "license": "MIT",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "docker:db:push": "dotenv -e .env.development.migrations yarn prisma db push",
    "docker:dev": "docker compose -f docker-compose.dev.yml up --build",
    "docker:dev:down": "docker compose -f docker-compose.dev.yml down",
    "lint": "next lint",
    "migrate:dev": "yarn prisma migrate dev",
    "migrate:prod": "yarn prisma migrate deploy",
    "prettier": "prettier --write .",
    "start": "next start",
    "static-checks": "yarn prettier && yarn lint && yarn typecheck",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.9.0",
    "@next-auth/prisma-adapter": "^1.0.3",
    "@prisma/client": "^4.1.1",
    "dotenv-cli": "^6.0.0",
    "next": "^12.2.2",
    "next-auth": "^4.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.5",
    "@types/react": "^18.0.15",
    "eslint": "^8.19.0",
    "eslint-config-next": "^12.2.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-material-ui": "^1.0.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "2.7.1",
    "prisma": "^4.1.1",
    "typescript": "^4.7.4"
  }
}
