{
  "name": "daily-roulette",
  "version": "0.2.0",
  "author": "Alex Radulescu",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:dev:down": "docker-compose -f docker-compose.dev.yml down",
    "lint": "next lint",
    "migrate": "dotenv -e .env.migrations -- yarn prisma migrate dev",
    "typecheck": "tsc --noEmit",
    "prettier": "prettier --write .",
    "static-checks": "yarn prettier && yarn lint && yarn typecheck",
    "generate": "yarn generate:prisma && yarn generate:nexus",
    "generate:prisma": "yarn prisma generate",
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "generate:nexus": "ts-node --transpile-only -P nexus.tsconfig.json pages/api"
  },
  "dependencies": {
    "@apollo/client": "^3.5.9",
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.1",
    "@mui/material": "^5.2.3",
    "@next-auth/prisma-adapter": "^1.0.0",
    "@prisma/client": "^3.9.2",
    "apollo-server-micro": "^3.6.3",
    "express": "^4.17.3",
    "graphql": "^16.3.0",
    "graphql-scalars": "^1.14.1",
    "graphql-tag": "^2.12.6",
    "micro": "^9.3.4",
    "micro-cors": "^0.1.1",
    "next": "^12.0.7",
    "next-auth": "^4.0.2",
    "nexus": "^1.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.18",
    "@types/react": "^17.0.39",
    "eslint": "^8.4.0",
    "eslint-config-next": "^12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-material-ui": "^1.0.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "2.5.1",
    "prisma": "^3.9.2",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  }
}
